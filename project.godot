DebugUtils="*res://Utils/DebugUtils.gd"
FileBankUtils="*res://Utils/FileBankUtils.gd"
TagsLibrary="*res://ecs/TagsLibrary.gd"
EntitiesUtils="*res://Utils/EntitiesUtils.gd"
AnimationUtils="*res://Utils/AnimationUtils.gd"
FontChoice="*res://font/FontChoice.gd"
NodeUtils="*res://Utils/NodeUtils.gd"
Fade="*res://Utils/Fade.tscn"
Scene_transition_data="*res://Utils/Scene_transition_data.gd"
CameraUtils="*res://Utils/CameraUtils.gd"
TouchUtils="*res://Utils/TouchUtils.gd"

[debug]

settings/fps/force_fps=60
shapes/collision/shape_color=Color( 0, 0.6, 0.701961, 0.501961 )

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "Grid" )

