[application]

config/name="jeu"
run/main_scene="res://Src/Outgame/Signin.tscn"
config/icon="res://Assets/Textures/GUI/icon.png"

[autoload]

ECS="*res://ecs/World.gd"
ComponentsLibrary="*res://ecs/Components/Library.gd"
SystemsLibrary="*res://ecs/Systems/Library.gd"
ArrayUtils="*res://Utils/Array_Utils.gd"
MoveUtils="*res://Utils/MoveUtils.gd"
RandomUtils="*res://Utils/RandomUtils.gd"
